{
  "version": 2,
  "name": "dayoff-API",
  "builds": [
    { "src": "index.js", "use": "@vercel/node" },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/api(.*)", "dest": "/index.js" },
    {
      "src": "/(.*)",
      "dest": "public/index.html"
    }
  ]
}
